{
    "name":"monitoring-server",
    "description":"Icinga Monitoring Host",
    "json_class":"Chef::Role",
    "default_attributes":{
        "apache2":{
            "listen_ports":[
                "80",
                "443"
            ]
        },
        "bigpoint": {
          "meta.pids": [
            "0792"
          ],
          "meta.unixnames": [
            "admin"
          ]
        },
        "check_mk": {
            "metadata": {
              "enabled": true,
              "name": "bigpoint"
            },
            "manual_checks": {
              "hosts": [
                { 
                  "hostname": "localhost",
                  "ipaddress": "127.0.0.1",
                  "environment": "_default",
                  "hosttags": ["snmp"],
                  "default_hosttag": "snmp"
      		      }
                  ],
              "checks": [
                {
                  "hostnames": ["localhost"],
                  "check_type": "df",
                  "check_item": "/",
                  "check_warn": 80,
                  "check_crit": 90
                }
              ]
            }
        }
    },
    "override_attributes":{
    },
    "chef_type":"role",
    "run_list":[
        "recipe[apache2]",
        "recipe[apache2::mod_ssl]",
        "recipe[icinga::server]",
        "recipe[icinga::client]"
    ],
    "env_run_lists":{
    },
